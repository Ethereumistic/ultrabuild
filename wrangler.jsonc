{
    "$schema": "node_modules/wrangler/config-schema.json",
    "name": "ultrabuild",
    "main": ".open-next/worker.js",
    "compatibility_date": "2025-01-01",
    "compatibility_flags": [
        "nodejs_compat"
    ],
    // Use the new Workers Assets to host the static frontend files
    "assets": {
        "directory": ".open-next/assets",
        "binding": "ASSETS"
    }
}